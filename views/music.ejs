<%- include('partials/h.admin.ejs') %>

<h1>Music</h1>
<div class="addsong">
  <a href="/music/new"><button class="addsong">ADD SONG</button></a>
</div>
<% music.forEach(function(music){ %>
<div id="primary-second-section">
    <div class="most-popular-items">
      <div class="music-icon-section">
        <img src="<%= music.image %>"/>
        <i class="fas fa-caret-right"></i>
        <h2><%= music.title %></h2>
      </div>
      <div class="music-artist">
        <h2><%= music.name %></h2>
      </div>
      <div>
        <a href="/music/view/<%= music._id %>"><button>View</button></a>
        <a href="/music/<%= music._id %>"><button>Edit</button></a>
        <form id = 'delete-form' action="/music/<%= music._id %>?_method=DELETE" method="POST">
          <a href="/music/<%= music._id %>"><button>Delete</button></a>
        </form>
      </div>
    </div>
</div>
<% }); %>


<%- include('partials/footer.ejs') %>