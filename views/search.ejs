<%- include('partials/h.click.ejs') %>

<h1>Your search</h1>
<% if(music == '' ) { %>
  <div class="notsearch">
  <p>NOT YOUR SEARCH</p>
  </div>
<% } else { %>
  <ul class="card-list">
  <% music.forEach(function(music){ %>
  <li class="card">
    <a href="/homeclick/<%= music._id %>" class="title">
    <image src="<%= music.image %>" height="200px" />
    <div class="card-description">
      <a href="/homeclick/<%= music._id %>" class="title"><%= music.title %></a><br/>
      <a href="#" class="name"><%= music.name %></a>
    </div>
  </a>
  </li>
  <% }); %>
  </ul>
<% } %>


